#include<stdio.h>
void swap(long int *x, long int *y)
{
	int temp;
	temp=*x;
	*x=*y;
	*y=temp;
	printf("first value=%d\n",*x);
	printf("second value=%d\n",*y);
}
int main()
{
	long int a,b;
	printf("enter first number=");
	scanf("%d",&a);
	printf("enter second number=");
	scanf("%d",&b);
	swap(&a,&b);
}
